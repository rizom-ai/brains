---
// Generic object renderer - fallback layout for any data structure
// This is the only layout provided by site-builder itself
// All other layouts should come from plugins

export type Props = Record<string, unknown>;

const props = Astro.props;
---

<section class="object-layout p-4">
  <div class="max-w-4xl mx-auto">
    <pre class="bg-gray-100 p-4 rounded overflow-x-auto">
      <code>{JSON.stringify(props, null, 2)}</code>
    </pre>
  </div>
</section>

<style>
  .object-layout {
    font-family: monospace;
  }
</style>